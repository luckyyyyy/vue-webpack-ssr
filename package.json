{
  "name": "vue-webpack-ssr",
  "version": "1.0.0",
  "description": "This repo is a boilerplate for vue-webpack-ssr project. You could use it as a base to build your own web app.",
  "author": "William Chan <root@williamchan.me>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --config build/client/webpack.dev.conf.js",
    "dev:ssr": "node --inspect build/server/dev-server",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config build/client/webpack.prod.conf --hide-modules",
    "build:ssr": "node build/server/build",
    "start": "node server",
    "lint": "eslint --ext .js,.vue,.jsx --cache --cache-location node_modules/.cache/.eslintcache src",
    "lint:style": "stylelint src/**/*.{less,vue} --fix",
    "prebuild": "npm run check",
    "prebuild:ssr": "npm run check",
    "check": "node build/check-version"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-js": "3",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.19.1",
    "lodash": "^4.17.10",
    "normalize.css": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.3",
    "serve-favicon": "^2.5.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.1.1",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^0.10.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.3",
    "@vue/babel-preset-jsx": "^1.0.0-beta.3",
    "ansi-styles": "^4.0.0",
    "autoprefixer": "^9.1.0",
    "babel-loader": "^8.0.5",
    "cache-loader": "^2.0.1",
    "chalk": "^2.4.1",
    "compression-webpack-plugin": "^1.1.11",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "eruda": "^1.5.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
