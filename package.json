    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^3.1.3",
    "yargs": "^13.2.4"
  }
}
