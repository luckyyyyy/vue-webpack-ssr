    "sass-loader": "^7.1.0",
    "semver": "^5.5.0",
    "shelljs": "^0.8.2",
    "stylelint": "^9.4.0",
    "stylelint-bare-webpack-plugin": "^0.1.3",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-no-unsupported-browser-features": "^3.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
